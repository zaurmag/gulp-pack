{
  "name": "gulp-pack",
  "version": "1.0.1",
  "description": "My starter project for web development",
  "main": "gulpfile.js",
  "scripts": {
    "Start": "gulp default",
    "Minify": "gulp pug:build style:build js:build --minify true",
    "MinifyHtml": "gulp pug:build --minify-html true",
    "MinifyJs": "gulp js:build --minify-js true",
    "MinifyCss": "gulp style:build --minify-css true",
    "Start Ftp": "gulp default_ftp --ftp true",
    "Ftp": "gulp ftp:build --ftp true",
    "lint:scripts": "./node_modules/.bin/eslint src/**/*.js",
    "lintFix:scripts": "./node_modules/.bin/eslint src/**/*.js --fix",
    "zip": "gulp zip:build",
    "clean": "gulp clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zaurmag/gulp-pack.git"
  },
  "keywords": [
    "gulp",
    "web-developer",
    "html",
    "css"
  ],
  "author": "Zaur Magomedov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-private-methods": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/polyfill": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/runtime": "7.4.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "bem-tools-core": "bem-tools/bem-tools-core",
    "bem-tools-create": "bem-tools/bem-tools-create",
    "browser-sync": "2.27.10",
    "critical": "4.0.1",
    "del": "6.1.1",
    "emitty": "^1.4.0",
    "eslint": "8.18.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "8.0.0",
    "gulp-changed": "4.0.3",
    "gulp-cheerio": "1.0.0",
    "gulp-csso": "4.0.1",
    "gulp-data": "1.3.1",
    "gulp-eslint": "6.0.0",
    "gulp-favicons": "3.0.0",
    "gulp-if": "3.0.0",
    "gulp-load-plugins": "2.0.7",
    "gulp-plumber": "1.2.1",
    "gulp-pug": "5.0.0",
    "gulp-pxtorem": "3.0.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svg-sprite": "1.5.0",
    "gulp-tinypng": "1.0.2",
    "gulp-watch": "5.0.1",
    "gulp-webp": "^4.0.1",
    "gulp-zip": "5.1.0",
    "gulp.spritesmith": "6.13.0",
    "node-sass": "9.0.0",
    "vinyl-ftp": "0.6.1",
    "webpack": "5.76.0",
    "webpack-stream": "7.0.0",
    "yargs": "17.5.1"
  },
  "dependencies": {
    "bootstrap": "5.2.0",
    "enquire.js": "2.1.6",
    "inputmask": "5.0.7",
    "lozad": "^1.16.0",
    "svg4everybody": "2.1.9"
  }
